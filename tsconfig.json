{
	"compilerOptions": {
		"target": "ES2015",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": ["*", "*/*", "*/*/*", "*/*/*/*", "*/*/*/*/*", "*/*/*/*/*/*", "*/*/*/*/*/*/*", "*/*/*/*/*/*/*/*", "*/*/*/*/*/*/*/*/*", "*/*/*/*/*/*/*/*/*/*"]
}
