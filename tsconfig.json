		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": true,
